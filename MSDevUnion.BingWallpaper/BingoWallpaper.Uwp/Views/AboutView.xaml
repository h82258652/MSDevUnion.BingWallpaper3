<local:ViewBase x:Class="BingoWallpaper.Uwp.Views.AboutView"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                xmlns:local="using:BingoWallpaper.Uwp.Views"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                xmlns:controls="using:BingoWallpaper.Uwp.Controls"
                mc:Ignorable="d">
    <Page.Resources>
        <ResourceDictionary Source="/Styles/BingoWallpaperCheckBoxStyle.xaml"></ResourceDictionary>
    </Page.Resources>
    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <CheckBox Style="{StaticResource BingoWallpaperCheckBoxStyle}"
                  HorizontalAlignment="Center"
                  x:Name="CheckBox"
                  Content="Hello world"
                  VerticalAlignment="Center"></CheckBox>
        <CheckBox Margin="250,0,0,0"
                  HorizontalAlignment="Center"
                  Content="Hello World2"></CheckBox>
        <Button Content="Test"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Click="ButtonBase_OnClick"></Button>
    </Grid>
</local:ViewBase>